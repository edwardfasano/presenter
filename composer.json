{
    "name": "hemp/presenter",
    "description": "Easy Model Presenters in Laravel",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "illuminate/support": "~5.2|~5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0@dev",
        "phpunit/phpunit": "~4.0",
        "illuminate/database": "~5.2|~5.3",
        "graham-campbell/testbench-core": "^1.1",
        "graham-campbell/testbench": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Hemp\\Presenter\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hemp\\Tests\\Presentable\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
